<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.net.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Group xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.book.CheckoutController">
	<children>
		<AnchorPane id="AnchorPane" layoutX="10.0" layoutY="10.0"
			prefHeight="252.0" prefWidth="350" stylesheets="/style/PopUp.css">
			<children>
				<BorderPane prefHeight="250.0" prefWidth="340.0"
					stylesheets="/style/MainStyle.css" AnchorPane.bottomAnchor="0.0"
					AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
					AnchorPane.topAnchor="0.0">
					<center>
						<Pane prefHeight="250.0" prefWidth="340.0"
							BorderPane.alignment="CENTER">
							<children>
								<GridPane layoutX="14.0" layoutY="68.0">
									<columnConstraints>
										<ColumnConstraints hgrow="SOMETIMES"
											minWidth="10.0" prefWidth="100.0" />
										<ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES"
											minWidth="10.0" prefWidth="100.0" />
										<ColumnConstraints hgrow="SOMETIMES"
											maxWidth="146.0" minWidth="10.0" prefWidth="112.0" />
									</columnConstraints>
									<rowConstraints>
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
										<RowConstraints maxHeight="30.0" minHeight="10.0"
											prefHeight="30.0" vgrow="SOMETIMES" />
									</rowConstraints>
									<children>
										<MenuButton fx:id="mbtMember" mnemonicParsing="false"
											onMouseClicked="#onClickedMenuButtonMember" prefHeight="25.0"
											prefWidth="312.0" text="Select member" GridPane.columnSpan="3">
											<items>
												<MenuItem mnemonicParsing="false">
													<graphic>
														<TextField fx:id="txtSearchMember"
															onKeyReleased="#onKeyReleasedTextFieldSearch" prefHeight="25.0"
															prefWidth="240.0" />
													</graphic>
												</MenuItem>
												<MenuItem mnemonicParsing="false">
													<graphic>
														<TableView fx:id="tblMember" onMouseClicked="#onClickTableMember"
															prefHeight="150.0" prefWidth="250.0">
															<columnResizePolicy>
																<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
															</columnResizePolicy>
															<columns>
																<TableColumn fx:id="clmMemberId" prefWidth="75.0"
																	text="ID" visible="false" />
																<TableColumn fx:id="clmFirstName" prefWidth="102.0"
																	text="First Name" />
																<TableColumn fx:id="clmLastName" prefWidth="100.0"
																	text="Last Name" />
															</columns>
														</TableView>
													</graphic>
												</MenuItem>
											</items>
										</MenuButton>
										<Label text="ISBN:" GridPane.columnSpan="2"
											GridPane.rowIndex="1" />
										<TextField fx:id="txtIsbn" disable="true"
											GridPane.columnSpan="3" GridPane.rowIndex="2" />
										<Button fx:id="btnOK" mnemonicParsing="false"
											onAction="#onClickButtonOK" prefHeight="25.0" prefWidth="99.0"
											text="OK" GridPane.columnIndex="1" GridPane.rowIndex="4" />
									</children>
								</GridPane>
								<Label layoutX="92.0" layoutY="12.0" prefHeight="39.0"
									prefWidth="176.0" text="Checkout Book" textAlignment="CENTER">
									<font>
										<Font name="System Bold" size="24.0" />
									</font>
								</Label>
								<Button fx:id="btnClose" layoutX="319.0" layoutY="-10.0"
									mnemonicParsing="false" onAction="#onClickButtonClose"
									prefHeight="34.0" prefWidth="32.0" stylesheets="@../../style/btnOnText.css" />
							</children>
						</Pane>
					</center>
				</BorderPane>
			</children>
		</AnchorPane>
	</children>
</Group>
